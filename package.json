{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prebuild": "npx amplify push",
    "build": "npx expo build:web",
    "start": "npx expo start",
    "debug": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "eject": "npx expo eject",
    "test": "jest",
    "backup": "export z=$(date +%Y%m%d%H%M%S); mkdir backup/$z; cd backup/$z; dynamodump export-all-data --region=us-east-1"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "setupFilesAfterEnv": [
      "jest-expo/jest-preset.js",
      "@testing-library/react-native/jest-preset",
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@sentry|sentry-expo|Linking|aws-amplify-react-native|@aws-sdk|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.9.8",
    "@material-ui/pickers": "^3.2.10",
    "@react-native-community/masked-view": "0.1.6",
    "@react-native-community/netinfo": "^5.6.2",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@sentry/react-native": "^1.3.7",
    "@testing-library/jest-dom": "^5.5.0",
    "@types/googlemaps": "^3.39.3",
    "@types/jest": "^25.2.1",
    "@types/react-test-renderer": "^16.9.2",
    "@zoomus/websdk": "^1.7.2",
    "Dante2": "^0.5.0-rc25",
    "asset": "^0.4.13",
    "aws-amplify": "^3.0.5",
    "aws-amplify-react-native": "^4.0.4",
    "aws-sdk": "^2.640.0",
    "babel-preset-expo": "^8.1.0",
    "emotion-theming": "^10.0.27",
    "expo": "^37.0.9",
    "google-maps-react": "^2.0.6",
    "is-absolute-url": "^3.0.3",
    "jquery": "^3.4.1",
    "moment": "^2.15.2",
    "native-base": "2.13.8",
    "react": "^16.13.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-draft-wysiwyg": "^1.14.4",
    "react-google-maps": "^9.4.5",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-calendars": "^1.264.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "^0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-progress-wheel": "^1.0.5",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "~2.2.0",
    "react-native-scripts": "^2.0.1",
    "react-native-tags-input": "^1.0.7",
    "react-native-web": "^0.11.7",
    "react-native-web-maps": "^0.2.0",
    "react-navigation": "^4.3.1",
    "react-navigation-drawer": "^2.4.4",
    "react-navigation-stack": "^2.3.1",
    "react-navigation-tabs": "^2.8.4",
    "react-places-autocomplete": "^7.2.1",
    "request": "^2.88.2",
    "resize-observer-polyfill": "^1.5.1",
    "sentry-expo": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@expo/webpack-config": "^0.12.4",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/react": "^10.0.3",
    "@testing-library/react-native": "^5.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.15",
    "babel-jest": "^25.5.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "dynamic-cdn-webpack-plugin": "^5.0.0",
    "expo-cli": "^3.20.3",
    "isomorphic-fetch": "^2.2.1",
    "jest-expo": "^37.0.0",
    "jest-fetch-mock": "^3.0.3",
    "module-to-cdn": "^3.1.5",
    "node-fetch": "^2.6.0",
    "react-test-renderer": "^16.13.1",
    "readline": "^1.3.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
